<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600" prefWidth="800" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="implicaspection.MainController">
   <children>
      <Button fx:id="buttonRandom" layoutX="8.0" layoutY="538.0" mnemonicParsing="false" onAction="#generateRandom" prefHeight="48.0" prefWidth="679.0" text="Your random number for this session: " />
      <Label fx:id="labelRandom" layoutX="691.0" layoutY="543.0" prefHeight="38.0" prefWidth="95.0" text="..." textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="28.0" />
         </font></Label>
      <VBox layoutX="72.0" layoutY="67.0" prefHeight="188.0" prefWidth="645.0">
         <children>
            <TextField fx:id="queryInput" prefHeight="0.0" prefWidth="182.0" promptText="Enter command" />
            <Button mnemonicParsing="false" onAction="#processInput" prefHeight="48.0" prefWidth="797.0" text="Enter Command" />
            <Label fx:id="queryOutput" prefHeight="161.0" prefWidth="667.0" text="Label">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
         </children>
      </VBox>
   </children>
</AnchorPane>
