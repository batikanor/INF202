      <Line endX="706.0" layoutX="127.0" layoutY="513.0" startX="-100.0" stroke="WHITE" strokeWidth="4.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />
      <Line endX="706.0" layoutX="102.0" layoutY="431.0" startX="-100.0" stroke="WHITE" strokeWidth="4.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />
