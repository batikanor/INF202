         <font>
            <Font name="System Bold" size="28.0" />
         </font></Label>
      <Button fx:id="buttonAdmin" layoutX="14.0" layoutY="445.0" mnemonicParsing="false" onAction="#openAdminPanel" prefHeight="50.0" prefWidth="150.0" text="Admin paneli iste" />
