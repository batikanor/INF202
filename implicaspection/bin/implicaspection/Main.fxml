<AnchorPane fx:id="mainAnchorPane" prefHeight="600" prefWidth="791.0" style="-fx-border-radius: 20; -fx-border-style: solid;" styleClass="root" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="implicaspection.MainController">
   <children>
      <Button fx:id="buttonRandom" layoutX="14.0" layoutY="536.0" mnemonicParsing="false" onAction="#generateRandom" prefHeight="50.0" prefWidth="200.0" text="Rastgele sayı oluştur" />
